#EXAMPLE ENVIRONMENT VARIABLES FOR VITE FRONTEND

# -------------------------------------
# Application Configuration
# -------------------------------------

# The base URL for your frontend app
VITE_APP_URL=https://localhost:5173

VITE_APP_NAME=SchedulerApp

# -------------------------------------
# API Configuration
# -------------------------------------

# Base URL for API requests
VITE_API_BASE_URL=https://api.example.com/v1

# Toggle to use mock data instead of real API
VITE_USE_MOCK=false

# -------------------------------------
# Feature Flags
# -------------------------------------

# Example feature toggles
VITE_FEATURE_NAV_BAR=false
VITE_FEATURE_EXPERIMENTAL_UI=false

# -------------------------------------
# Build / Debugging
# -------------------------------------

# Enable verbose logging in development
VITE_ENABLE_DEBUG_LOGS=true